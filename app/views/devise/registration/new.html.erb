<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name, :autofocus => true %>
  
<%= f.label :last_name %>
  <%= f.text_field :last_name %>
 
  <%= f.label :email %>
  <%= f.email_field :email %>
  
<%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :address2 %>
  <%= f.text_field :address2 %>
  
  <%= f.submit "Sign Up"%>
<% end %>
